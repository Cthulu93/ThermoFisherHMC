container_commands:
  01_syncdb:    
    command: "python manage.py syncdb --noinput"
    leader_only: true
  02_createadmin:
    command: "scripts/createadmin.py"
    leader_only: true
  03_collectstatic:
    command: "python manage.py collectstatic --noinput"

